te <- read.table("/srv/gsfs0/projects/levinson/transfer/Project_VAC_10942_B01_GRM_WGS.2015-09-18/Sample_320-FR-clone3/retro_0725/LINE/Sample_320-FR-clone3.LINE.GT2.cand.calls", sep="\t", header=F)
