dat1 <- read.table("/home/xwzhu/transfer/Project_VAC_10942_B01_GRM_WGS.2015-09-18/hg38/TPTN42/LINE/somatic.LINE.1.calls", header=T, sep="\t")
dat2 <- read.table("/home/xwzhu/transfer/Project_VAC_10942_B01_GRM_WGS.2015-09-18/hg38/TPTN42/LINE/somatic.LINE.0.calls", header=T, sep="\t")

z <- c()
for (i in 1:dim(dat1)[1])
   {
    if ( (max(dat1[i,4:54]) > 9) && ( (sum(dat1[i,4:28]>1) + sum(dat1[i,30:54]>1)) < 4) )
       {
        z <- rbind(z, dat1[i,])
       }
   }

subdat <- subset(dat1, dat1[,11] > 0)
x <- subdat[,11]
y <- c()
for (i in 1:20970)
   {
    y[i]<-sum(subdat[i,4:28]>0) + sum(subdat[i,30:54]>0)
   }
clone <- data.frame(x, y)
names(clone) <- c("x", "y")

ggplot(clone, aes(x=x, y=y)) +
   geom_jitter() +
   theme(axis.text=element_text(size=18),
       axis.title=element_text(size=24,face="bold")) +
   xlab("Number of support reads") +
   ylab("Number of clones")
ggsave("/home/xwzhu/levinson/MEI_plots/BG_clone16_pos.pdf",useDingbats=FALSE, width=7, height=7)

